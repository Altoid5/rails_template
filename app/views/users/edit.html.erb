<h1>Edit Profile</h1>

<%= form_with(model: @user, local: true) do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.submit "Update" %>
  </div>
<% end %>